class Solution {
    public ListNode removeNthFromEnd(ListNode head, int n) {
        ListNode i=new ListNode();
        i.next=head;
        ListNode j=head;
        head=i;
        int m=n;
        while(j!=null)
        {
            if(m==0)
            {
                i=i.next;
                m=1;
            }
            j=j.next;
            m--;
        }
        i.next=i.next.next;
        return head.next;
    }
}
