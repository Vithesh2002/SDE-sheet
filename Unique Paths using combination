class Solution {
    public int uniquePaths(int m, int n1) {
        double n=m+n1-2;
        double r=Math.min(m,n1)-1;
        double did=1,div=1;
        double res=1.0;
        for(int i=0;i<r;i++)
        {
            did*=(n-i);
            div*=(r-i);
        }
        return (int)(did/div);
    }
}
